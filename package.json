    "@vue/compiler-sfc": "^3.3.4",
    "eslint": "^8.48.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "typescript": "5.2",
    "vite": "^4.4.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-windicss": "^1.9.1",
    "vue-tsc": "^1.8.8"
