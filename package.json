    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.4.0",
    "graphql": "^16.8.0",
    "graphql-ws": "^5.14.0",
    "idb-keyval": "^6.2.1",
    "safari-14-idb-fix": "^3.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
