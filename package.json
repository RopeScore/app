    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.17.0",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vite-plugin-pwa": "^0.11.2",
    "vite-plugin-windicss": "^1.3.0",
    "vue-tsc": "^0.2.3"
  }
