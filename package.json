    "@vueuse/core": "^6.1.0",
    "graphql": "^15.5.2",
    "idb-keyval": "^5.1.3",
    "safari-14-idb-fix": "^1.0.4",
    "uuid": "^8.3.2",
    "vue": "^3.2.8",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.0",
